<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.miditest.MainActivity">

    <Button
        android:id="@+id/buttonPlayNote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Repeat"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonNoteGen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="8dp"
        android:text="new tone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/button1"
        android:text="@string/btn1"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/buttonPlayNote"
        app:layout_constraintRight_toLeftOf="@+id/button2"
        android:layout_marginRight="32dp" />

    <Button
        android:id="@+id/button2"
        android:text="@string/btn2"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/buttonPlayNote"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/button3"
        android:text="@string/btn3"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/buttonPlayNote"
        app:layout_constraintLeft_toRightOf="@+id/button2"
        android:layout_marginLeft="32dp" />

    <Button
        android:id="@+id/button4"
        android:text="@string/btn4"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/button1"
        app:layout_constraintRight_toLeftOf="@+id/button5"
        android:layout_marginRight="32dp" />

    <Button
        android:id="@+id/button5"
        android:text="@string/btn5"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/button6"
        android:text="@string/btn6"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:layout_constraintLeft_toRightOf="@+id/button5"
        android:layout_marginLeft="32dp" />

    <Button
        android:id="@+id/button7"
        android:text="@string/btn7"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        app:layout_constraintRight_toLeftOf="@+id/button8"
        android:layout_marginRight="32dp" />

    <Button
        android:id="@+id/button8"
        android:text="@string/btn8"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/button9"
        android:text="@string/btn9"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        app:layout_constraintLeft_toRightOf="@+id/button8"
        android:layout_marginLeft="32dp" />

    <Button
        android:id="@+id/button10"
        android:text="@string/btn10"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        app:layout_constraintRight_toLeftOf="@+id/button11"
        android:layout_marginRight="32dp" />

    <Button
        android:id="@+id/button11"
        android:text="@string/btn11"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <Button
        android:id="@+id/button12"
        android:text="@string/btn12"
        android:textAllCaps="false"
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/button9"
        app:layout_constraintLeft_toRightOf="@+id/button11"
        android:layout_marginLeft="32dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="64dp"
        app:layout_constraintTop_toBottomOf="@+id/button11" />

</android.support.constraint.ConstraintLayout>